<%
items = [
  `echo $(brew --prefix coreutils)/libexec/gnubin`.strip,
  `echo $(brew --prefix findutils)/libexec/gnubin`.strip,
  `echo $(brew --prefix diffutils)/bin`.strip,
  `echo $(brew --prefix openssl)/bin`.strip,
  `echo $(brew --prefix grep)/libexec/gnubin`.strip,
  `echo $(brew --prefix make)/libexec/gnubin`.strip,
  `echo $(brew --prefix cmake)/bin`.strip,
  `echo $(brew --prefix gnu-tar)/libexec/gnubin`.strip,
  `echo $(brew --prefix gnu-sed)/libexec/gnubin`.strip,
  `echo $(brew --prefix php)/bin`.strip,
]
%>
export PATH=<%= items.join(':') %>:$PATH
