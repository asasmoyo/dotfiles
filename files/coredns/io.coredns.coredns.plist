<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-/Apple/DTD PLIST 1.0/EN" "http:/www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>Label</key>
    <string>io.coredns.coredns</string>
    <key>KeepAlive</key>
    <true/>
    <key>RunAtLoad</key>
    <true/>
    <key>ProgramArguments</key>
    <array>
      <string>/opt/coredns/coredns</string>
      <string>-conf</string>
      <string>/opt/coredns/config</string>
      <string>-pidfile</string>
      <string>/var/run/coredns.pid</string>
    </array>
    <key>StandardErrorPath</key>
    <string>/var/log/coredns.err.log</string>
    <key>StandardOutPath</key>
    <string>/var/log/coredns.out.log</string>
  </dict>
</plist>
